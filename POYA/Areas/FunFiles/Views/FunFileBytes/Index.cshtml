@model IEnumerable<POYA.Areas.FunFiles.Models.FunFileByte>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @(Html.DisplayNameFor(model => model.FileSHA256))
            </th>
            <th>
                @(Html.DisplayNameFor(model => model.FirstUploaderId))
            </th>
            <th>
                @(Html.DisplayNameFor(model => model.DOUploading))
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @(Html.DisplayFor(modelItem => item.FileSHA256))
            </td>
            <td>
                @(Html.DisplayFor(modelItem => item.FirstUploaderId))
            </td>
            <td>
                @(Html.DisplayFor(modelItem => item.DOUploading))
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>
