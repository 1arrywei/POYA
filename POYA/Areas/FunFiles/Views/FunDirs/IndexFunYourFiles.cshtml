@model IEnumerable<POYA.Areas.FunFiles.Models.FunYourFile>

@{

}

 @foreach (var item in Model) 
{
    <tr>
        <td>
            <a asp-action="Details" asp-route-Id='@(item.Id)' asp-controller="FunYourFiles">
                @(Html.DisplayFor(modelItem => item.Name))
                <span style="color: red;font-size: 70%">@(Localizer["file"])</span>
            </a>
        </td>
        <td>
            @(item.DOUploading.LocalDateTime)
        </td>
        <td>
            <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
            <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
        </td>
    </tr>
}