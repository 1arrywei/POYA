}
<script>

    $(document).ready(()=>{
        _LoginPartial_Initial_Js();
    });
    
function _LoginPartial_Initial_Js(){

    $(document).ready(()=>{
        $.contextMenu({
            selector: '.NavAvatar',
            items: {
                'Profile': {
                    name: '@(Html.Raw( Localizer["Basic Profile"]))',
                    callback: (itemKey, opt, e)=> { 
                        window.location.href=`@(Url.Content("/Identity/Account/Manage"))`; 
                    }    
                },
                'UserInfo': {
                    name: '@(Html.Raw( Localizer["Your info"]))',
                    callback: (itemKey, opt, e)=> { 
                        window.location.href=`@(Url.Content("/Identity/Account/Manage/UserInfo"))`; 
                    }    
                },
                'personal-data': {
                    name: '@(Html.Raw( Localizer["Your data"]))',
                    callback: (itemKey, opt, e)=> { 
                        window.location.href=`@(Url.Content("/Identity/Account/Manage/PersonalData"))`; 
                    }    
                },
                'change-password': {
                    name: '@(Html.Raw( Localizer["Change password"]))',
                    callback: (itemKey, opt, e)=> { 
                        window.location.href=`@(Url.Content("/Identity/Account/Manage/ChangePassword"))`; 
                    }    
                },
                'poya-message': {
                    name: '@(Html.Raw( Localizer["Message"]))',
                    callback: (itemKey, opt, e)=> { 
                        window.location.href=`@(Url.Content("/Identity/Account/Manage/POYAMessage"))`; 
                    }    
                }
            },
        }); 
    });
}

</script>